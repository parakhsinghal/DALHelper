<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
    <PackageId>ThinkingCog.DALHelper.SQLServer</PackageId>
    <Authors>Parakh Singhal</Authors>
    <Company>ThinkingCog</Company>
    <Product>ThinkingCog.DALHelper.SQLServer</Product>
    <Description>This nuget package helps retrieve and key in data into an instamce of SQL Server. The package can be used with any application that is based on .Net Core 5.0 and above.</Description>
    <Copyright>Parakh Singhal</Copyright>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/parakhsinghal/DALHelper</PackageProjectUrl>
    <RepositoryUrl>https://github.com/parakhsinghal/DALHelper</RepositoryUrl>
    <RepositoryType>Git</RepositoryType>
    <PackageTags>DAL Helper; SQL Server; Facade Design Pattern; ADO.Net</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Data.SqlClient" Version="3.0.0" />
  </ItemGroup>

</Project>
